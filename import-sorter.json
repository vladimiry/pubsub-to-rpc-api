{
  "root": true,
  "bracketSpacing": false,
  "sortImportsBy": "names",
  "groupRules": [
    {},
    "^(src/|test/|\\./|\\.\\./)"
  ],
  "wrappingStyle": {
    "maxBindingNamesPerLine": 0,
    "maxDefaultAndBindingNamesPerLine": 0,
    "maxExportNamesPerLine": 0,
    "maxNamesPerWrappedLine": 0,
    "ignoreComments": false
  },
  "keepUnused": [
    ".*"
  ]
}
